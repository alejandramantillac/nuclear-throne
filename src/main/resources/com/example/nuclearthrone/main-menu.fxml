<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.nuclearthrone.MainMenu"
            prefHeight="600.0" prefWidth="800.0">

    <VBox alignment="CENTER" spacing="15.0" translateX="200.0" translateY="400.0">
        <Button fx:id="playButton" text="Play" prefWidth="300.0" />
        <Button fx:id="settingsButton" text="Settings" prefWidth="300.0" />
        <Button fx:id="creditsButton" text="Credits" prefWidth="300.0" />
        <Button fx:id="quitButton" text="Quit" prefWidth="300.0" />
    </VBox>

    <stylesheets>
        <URL value="@menu.css" />
    </stylesheets>

</AnchorPane>
